{
  "expo": {
    "name": "Pickletour.vn Trọng tài",
    "slug": "pickletour-app-ref",
    "version": "1.0.2",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "pickletourapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "requireFullScreen": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.pkt.ref",
      "buildNumber": "3",
      "infoPlist": {
        "NSCameraUsageDescription": "App dùng camera để quét mã, chụp ảnh hồ sơ.",
        "NSPhotoLibraryUsageDescription": "App cần truy cập ảnh để bạn chọn avatar.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.pkt.ref",
      "versionCode": 12
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6e3ab74d-e148-4b97-9b08-4e44cb585894"
      }
    }
  }
}
